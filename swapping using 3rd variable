using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace swapping_using_3rd_variable
{
    class Program
    {
        static void Main(string[] args)
        {
            int a = 10, b = 20;
            int t;
            t = a;
            a = b;
            b = t;
            Console.WriteLine("a = " + a);
            Console.WriteLine("b = " + b);
            Console.ReadLine();
        }
    }
}
